{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe 生成活动文件",
            "command": "C:\\TDM-GCC-64\\bin\\g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",
                "-std=c++20",
                "-I",
                "C:\\TDM-GCC-64\\include",
                "C:\\Lib\\Cpp\\ta-lib\\c\\include",
                "${fileDirname}\\..\\include",
                "-L",
                "C:\\TDM-GCC-64\\lib",
                "C:\\Lib\\Cpp\\ta-lib\\c\\lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ]
}